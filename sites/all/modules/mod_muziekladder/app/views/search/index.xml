<?xml version="1.0" ?>
<snippets>
	<searchbox><![CDATA[
		]]>		
	</searchbox>
	<resultheader><![CDATA[
			
	<div class="row-fluid">
		<div class="span12">
			<div>##numFound##</div>

			<div class="pagination">
				<ul>
					##pagination##
				</ul>	
			</div>
		</div>	
	</div>	
		]]>
	</resultheader>
	<result>
		<![CDATA[	
        <div class="row-fluid search-result-row2">
            <div class="span2">
                <a class="gigdate" href="/gig/?##internallink##">##date##</a><br>
                <span class="location">##location##</span><br>
                <span class="city">##city##</span>

            </div>
            <div class="span10">
                <a href="/gig/?##internallink##">
                    ##title##
                </a><br>
                ##desc##
            </div>
        </div>
		]]>
	</result>
	<noresults>
		<![CDATA[
		<h1>Helaas</h1>
		<p>Helaas... geen resultaten gevonden voor <em>"##searchTerms##"</em>.</p>
		<p>Probeer het opnieuw met een andere zoekopdracht.</p>	
		]]>
	</noresults>
	<resultfooter><![CDATA[
			
	<div class="row-fluid">
		<div class="span12">
			<div class="pagination">
				<ul>
					##pagination##
				</ul>	
			</div>
		</div>
	</div>
		
		]]>
	</resultfooter>
</snippets>
